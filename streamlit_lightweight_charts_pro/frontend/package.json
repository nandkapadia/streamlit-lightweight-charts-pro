{
  "name": "streamlit_lightweight_charts_pro",
  "version": "0.1.2",
  "type": "module",
  "private": false,
  "license": "MIT",
  "dependencies": {
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "focus-trap-react": "^11.0.4",
    "jsdom": "^27.0.0",
    "lightweight-charts": "^5.0.8",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "streamlit-component-lib": "^2.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@playwright/test": "^1.56.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.6.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/chai": "^5.2.2",
    "@types/node": "^24.5.2",
    "@types/pixelmatch": "^5.2.6",
    "@types/pngjs": "^6.0.5",
    "@types/scheduler": "^0.26.0",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@vitejs/plugin-react": "^5.0.3",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "canvas": "^3.2.0",
    "chai": "^6.0.1",
    "compression-webpack-plugin": "^11.1.0",
    "eslint": "^9.36.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "http-server": "^14.1.1",
    "pixelmatch": "^7.1.0",
    "pngjs": "^7.0.0",
    "prettier": "^3.6.2",
    "terser-webpack-plugin": "^5.3.7",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.1",
    "vite": "^7.1.7",
    "vite-bundle-analyzer": "^1.2.3",
    "vitest": "^3.2.4",
    "webpack-bundle-analyzer": "^4.9.0"
  },
  "resolutions": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "typescript": "^5.9.2"
  },
  "overrides": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "typescript": "^5.9.2"
  },
  "scripts": {
    "start": "vite",
    "build": "vite build && node fix-html.cjs",
    "build:analyze": "vite build --mode analyze",
    "build:size": "vite build --mode analyze",
    "analyze:size": "npx vite-bundle-analyzer",
    "build:optimized": "vite build",
    "preview": "vite preview",
    "test": "NODE_OPTIONS='--max-old-space-size=4096 --expose-gc --max-semi-space-size=128' vitest run --pool=forks --poolOptions.forks.singleFork=true",
    "test:watch": "NODE_OPTIONS='--max-old-space-size=2048 --expose-gc' vitest",
    "test:all": "npm run test:unit && npm run test:components && npm run test:plugins && npm run test:integration",
    "test:unit": "NODE_OPTIONS='--max-old-space-size=3072 --expose-gc --max-semi-space-size=128' vitest run src/__tests__/unit src/__tests__/utils src/__tests__/hooks --pool=forks",
    "test:components": "NODE_OPTIONS='--max-old-space-size=3072 --expose-gc --max-semi-space-size=128' vitest run src/__tests__/components --pool=forks",
    "test:plugins": "NODE_OPTIONS='--max-old-space-size=3072 --expose-gc --max-semi-space-size=128' vitest run src/__tests__/plugins --pool=forks",
    "test:integration": "NODE_OPTIONS='--max-old-space-size=3072 --expose-gc --max-semi-space-size=128' vitest run src/__tests__/integration --pool=forks",
    "test:coverage": "NODE_OPTIONS='--max-old-space-size=8192 --expose-gc --max-semi-space-size=256' vitest run --coverage --pool=forks --poolOptions.forks.singleFork=true",
    "test:ci": "NODE_OPTIONS='--max-old-space-size=2048 --expose-gc' vitest run --coverage",
    "test:visual": "NODE_OPTIONS='--max-old-space-size=2048 --expose-gc' vitest run --config vitest.visual.config.ts",
    "test:visual:watch": "NODE_OPTIONS='--max-old-space-size=2048 --expose-gc' vitest watch --config vitest.visual.config.ts",
    "test:visual:update": "UPDATE_SNAPSHOTS=true npm run test:visual",
    "test:visual:ui": "NODE_OPTIONS='--max-old-space-size=2048 --expose-gc' vitest --config vitest.visual.config.ts --ui",
    "test:e2e": "playwright test",
    "test:e2e:update": "playwright test --update-snapshots",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "type-check": "tsc --noEmit",
    "type-check:strict": "tsc --project tsconfig.strict.json --noEmit",
    "build:prod": "vite build",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx}\"",
    "format:fix": "prettier --write --fix \"src/**/*.{ts,tsx,js,jsx}\"",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx --fix",
    "lint:check": "eslint src --ext .ts,.tsx,.js,.jsx",
    "code-quality": "npm run format && npm run lint && npm run type-check",
    "precommit": "npm run code-quality"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "homepage": "."
}
